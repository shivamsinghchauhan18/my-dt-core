<launch>
	<arg name="veh" default="$(env VEHICLE_NAME)"/>
	<arg name="verbose" default="false"/>
	<arg name="demo_name" value="apriltag_detector"/>
	<arg name="force_opencv" default="false"/>

	<!-- start basic args -->
	<include file="$(find duckietown_demos)/launch/master.launch">
		<!-- Basic parameters -->
		<arg name="veh" value="$(arg veh)"/>
        <arg name="verbose" value="$(arg verbose)" />
        <arg name="demo_name" value="$(arg demo_name)"/>

		<!-- Camera -->
		<arg name="/camera/raw" value="true"/>
	</include>

	<!-- Also allow direct launching of the node for param control -->
	<include file="$(find image_processing)/launch/image_decoding.launch">
		<arg name="veh" value="$(arg veh)"/>
		<arg name="force_opencv" value="$(arg force_opencv)"/>
	</include>
</launch>
