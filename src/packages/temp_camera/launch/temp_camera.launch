<launch>
  <arg name="veh" default="blueduckie" />
  <group ns="$(arg veh)">
  <arg name="node_name" default="camera_node" />
  <node pkg="temp_camera" type="temp_camera_node.py" name="$(arg node_name)" output="screen" respawn="false">
      <param name="veh" value="$(arg veh)" />
    </node>
  </group>
</launch>
